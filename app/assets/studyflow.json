  "types": [
    {
      "name": "MarkdownString",
      "superClass": [
        "String"
      ]
    },
    {
      "name": "StartEvent",
      "extends": [
        "bpmn:StartEvent"
      ],
      "superClass": [
        "studyflow:Element"
      ],
      "properties": [
        {
          "name": "requiresConsent",
          "description": "User requires to give consent before participating.",
          "isAttr": true,
          "type": "Boolean",
          "default": false
        },
        {
          "name": "consentFormUrl",
          "description": "Link to the consent form. Leave empty if you want to use the default consent form.",
          "isAttr": true,
          "type": "String",
          "condition": {
            "language": "json",
            "body": {
              "studyflow:requiresConsent": true
            }
          }
        }
      ]
    },
    {
      "name": "EndEvent",
      "extends": [
        "bpmn:EndEvent"
      ],
      "superClass": [
        "studyflow:Element"
      ],
      "properties": [
        {
          "name": "hasRedirectUrl",
          "description": "Redirect the user to the given URL after completion.",
          "isAttr": true,
          "type": "Boolean"
        },
        {
          "name": "redirectUrl",
          "description": "Link to redirect the user upon completion.",
          "isAttr": true,
          "type": "String",
          "condition": {
            "language": "json",
            "body": {
              "studyflow:hasRedirectUrl": true
            }
          }
        }
      ]
    },
    {
      "name": "Element",
      "description": "Base class for all elements in the Studyflow.",
      "isAbstract": true,
      "superClass": [
        "bpmn:BaseElement"
      ],
      "properties": []
    },
    {
      "name": "Activity",
      "description": "Base class for all activities in the Studyflow.",
      "isAbstract": true,
      "superClass": [
        "bpmn:Task",
        "studyflow:Element"
      ],
      "properties": [
        {
          "name": "url",
          "description": "Link to the activity.",
          "isAttr": true,
          "type": "String"
        }
      ]
    },
    {
      "name": "CognitiveTest",
      "superClass": [
        "Activity"
      ],
      "properties": [
        {
          "name": "instrument",
          "description": "Instrument used for the test.",
          "isAttr": true,
          "type": "InstrumentEnum",
          "default": "custom"
        },
        {
          "name": "behaverseTask",
          "description": "Behaverse instrument used for the test.",
          "type": "BehaverseTaskEnum",
          "default": "undefined",
          "isAttr": true,
          "condition": {
            "language": "json",
            "body": {
              "studyflow:instrument": "behaverse"
            }
          }
        }
      ]
    },
    {
      "name": "Questionnaire",
      "superClass": [
        "Activity"
      ],
      "properties": []
    },
    {
      "name": "Instruction",
      "superClass": [
        "Questionnaire"
      ],
      "properties": [
        {
          "name": "text",
          "description": "Instruction text in markdown format.",
          "isAttr": true,
          "type": "MarkdownString"
        }
      ]
    },
    {
      "name": "RandomGateway",
      "superClass": [
        "bpmn:ExclusiveGateway",
        "studyflow:Element"
      ],
      "properties": [
        {
          "name": "algorithm",
          "description": "Algorithm used for the assignment.",
          "isAttr": true,
          "type": "AssignmentAlgorithmEnum",
          "default": "probabilistic"
        },
        {
          "name": "probabilityFunction",
          "description": "Probability mass function that defines the discrete distribution of the random assignment. Defaults to `uniform`.",
          "isAttr": true,
          "type": "String",
          "default": "uniform",
          "condition": {
            "language": "json",
            "body": {
              "studyflow:algorithm": "probabilistic"
            }
          }
        }
      ]
    },
    {
      "name": "Dataset",
      "description": "Base class for all datasets in the Studyflow.",
      "isAbstract": false,
      "extends": [
        "bpmn:DataStoreReference"
      ],
      "superClass": [
        "studyflow:Element"
      ],
      "properties": [
        {
          "name": "datasetFormat",
          "description": "Format of the dataset (BDM, BIDS, PsychDS, etc).",
          "isAttr": true,
          "type": "DatasetFormatEnum",
          "default": "other"
        },
        {
          "name": "bdmDataLevel",
          "description": "Level of the data (events, trials, summary, models).",
          "isAttr": true,
          "type": "BDMDataLevelEnum",
          "default": "events",
          "condition": {
            "language": "json",
            "body": {
              "studyflow:datasetFormat": "bdm"
            }
          }
        },
        {
          "name": "bidsDataType",
          "description": "BIDS data type.",
          "isAttr": true,
          "type": "BIDSDataTypeEnum",
          "default": "events",
          "condition": {
            "language": "json",
            "body": {
              "studyflow:datasetFormat": "bids"
            }
          }
        }
      ]
    }
  ]
}